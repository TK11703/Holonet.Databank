@inherits UserClaimsBase
<AuthorizeView>
	<h4>Available User Claims</h4>
	<table class="table">
		<thead>
			<tr>
				<th>Claim Type</th>
				<th>Value</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var claim in _claims)
			{
				<tr>
					<td>@claim.Type</td>
					<td>@claim.Value</td>
				</tr>
			}
		</tbody>
	</table>
</AuthorizeView>
