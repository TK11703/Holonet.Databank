{
    "version": "2.0",
    "functionTimeout": "00:05:00",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "Function": "Information",
            "Default": "Warning"
        }
    },
    "extensions": {
        "queues": {
            "messageEncoding": "none"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    }
}